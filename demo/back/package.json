{
    "name": "raInMemoryKWT-demo-back",
    "version": "0.1.0",
    "private": true,
    "description": "Backend part of the ra-in-memory-jwt demo",
    "scripts": {
        "start": "nodemon --watch src src/index.js --ext js,json,yml",
        "migrate:latest": "knex migrate:latest",
        "migrate:rollback": "knex migrate:rollback",
        "migrate:up": "knex migrate:up",
        "migrate:down": "knex migrate:down",
        "migrate:list": "knex migrate:list",
        "migrate:create": "knex migrate:make"
    },
    "dependencies": {
        "bcrypt": "5.0.0",
        "convict": "6.2.4",
        "jsonwebtoken": "9.0.0",
        "knex": "0.21.2",
        "knex-stringcase": "1.4.1",
        "koa": "2.13.0",
        "koa-bodyparser": "4.3.0",
        "koa-json-error": "3.1.2",
        "koa-router": "9.1.0",
        "koa-static": "5.0.0",
        "koa2-cors": "2.0.6",
        "owasp-password-strength-test": "1.3.0",
        "pg": "8.3.0",
        "signale": "1.4.0"
    },
    "devDependencies": {
        "nodemon": "2.0.4"
    }
}
